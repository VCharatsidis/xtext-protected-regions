<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="net.danieldietrich.protectedregions.feature"
      label="Xtext Protected Region Support"
      version="2.0.1.qualifier">

   <description>
      This feature provides protected region support for Xtext/Xtend 2.3.0.
See https://github.com/danieldietrich/xtext-protected-regions/
for more details on usage.
   </description>

   <copyright>
      Written by Daniel Dietrich and Hendy Irawan.
   </copyright>

   <license url="http://www.eclipse.org/legal/epl-v10.html">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.xtext.xbase.lib" version="2.3.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.xtend.lib" version="2.3.0" match="greaterOrEqual"/>
      <import plugin="com.google.inject" version="3.0.0" match="greaterOrEqual"/>
      <import plugin="org.slf4j.api" version="1.6.4" match="greaterOrEqual"/>
      <import plugin="javax.inject"/>
      <import plugin="org.eclipse.xtext.builder" version="2.3.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.xtext" version="2.3.0" match="greaterOrEqual"/>
      <import plugin="net.danieldietrich.protectedregions.core" version="2.0.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.jobs"/>
   </requires>

   <plugin
         id="net.danieldietrich.protectedregions.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.danieldietrich.protectedregions.xtext"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
